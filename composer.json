{
  "name": "nstwf/mysql-connection",
  "description": "Reactphp MySQL transactional connection wrapper",
  "version": "1.2.2",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Timur Sadullayev",
      "email": "nstwf.pro@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "react/mysql": "^0.5.7|^0.6"
  },
  "config": {
    "platform": {
      "php": "8.1.0"
    },
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^10"
  },
  "autoload": {
    "psr-4": {
      "Nstwf\\MysqlConnection\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nstwf\\MysqlConnection\\": "tests/src"
    }
  }
}